{
  "name": "kodtrol",
  "productName": "Kodtrol",
  "version": "0.1.0",
  "private": true,
  "description": "Code-based show control software",
  "homepage": "http://kodtrol.com",
  "repository": "https://github.com/H-s-O/Kodtrol",
  "bugs": "https://github.com/H-s-O/Kodtrol/issues",
  "main": "./build/main/kodtrol-main.js",
  "scripts": {
    "start": "npx electron -r @babel/register ./src/main/kodtrol-main.js",
    "dev": "npm run clear-build && parcel watch ./src/ui2/index.html ./src/audio/index.html --target electron --out-dir ./build --public-url ../",
    "clear-build": "npx rimraf ./build/*",
    "build-babel": "babel ./src/main --out-dir ./build/main && babel ./src/common --out-dir ./build/common",
    "build-parcel": "parcel build ./src/ui2/index.html ./src/audio/index.html --target electron --out-dir ./build --public-url ../ --no-source-maps --no-minify",
    "dist": "npx electron-builder build",
    "build-and-dist": "npm run clear-build && npm run build-babel && npm run build-parcel && npm run dist",
    "rebuild": "npx electron-rebuild",
    "generate-screenshots": "npm start -- --screenshots ../../dev/screenshots.js --project ./dev/demo_project.json"
  },
  "author": "HsO <hugostonge@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.22.3",
    "@blueprintjs/datetime": "^3.15.2",
    "@blueprintjs/select": "^3.11.2",
    "@laser-dac/core": "^0.4.1",
    "@laser-dac/draw": "^0.4.1",
    "@laser-dac/ether-dream": "^0.4.1",
    "@reecelucas/react-use-hotkeys": "^1.1.2",
    "ace-builds": "^1.4.7",
    "audio-waveform-svg-path": "^1.0.2",
    "bootstrap": "^4.4.1",
    "bootstrap-sass": "^3.3.7",
    "bootstrap.native": "^2.0.21",
    "bootswatch": "^3.3.7",
    "chokidar": "^2.1.2",
    "classnames": "^2.2.5",
    "color": "^3.0.0",
    "css-electron-reset": "^1.0.0",
    "dmx": "^0.2.4",
    "domready": "^1.0.8",
    "eases": "^1.0.8",
    "electron-redux": "^1.4.0",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "howler": "^2.1.1",
    "jzz": "^1.0.2",
    "lodash": "^4.17.15",
    "midi": "^1.0.0",
    "object-hash": "^1.3.1",
    "osc": "^2.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-color": "^2.18.0",
    "react-connect-context": "^1.1.0",
    "react-dom": "^16.5.2",
    "react-redux": "^7.1.3",
    "react-split-pane": "^0.1.77",
    "react-splitter-layout": "^3.0.0",
    "redux": "^3.7.2",
    "redux-observers": "^2.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1",
    "uniqid": "^4.1.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "electron": "^7.1.12",
    "electron-builder": "^22.4.1",
    "electron-rebuild": "^1.10.0",
    "less": "^3.11.1",
    "node-abi": "^2.15.0",
    "parcel-bundler": "^1.12.4",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^3.0.2"
  }
}
